{
  "name": "@dependabit/plugins",
  "version": "0.1.4",
  "description": "Plugin registry and loader for extensible access methods",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsgo -p tsconfig.json",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/index.ts",
    "type-check": "tsgo --noEmit -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "plugins",
    "registry",
    "extensibility"
  ],
  "license": "MIT"
}
